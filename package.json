{
  "name": "nuxt-1.0-test",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Jakob Viskum Damgaard <jakob.viskum.damgaard@gmail.com>",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "analyze": "cross-env NODE_ENV=production nuxt build --analyze",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "npm run generate && npm run functions:build",
    "functions": "netlify-lambda serve functions",
    "functions:build": "cross-env NODE_ENV=production netlify-lambda build functions"
  },
  "dependencies": {
    "@nuxtjs/icon": "latest",
    "@nuxtjs/manifest": "latest",
    "@nuxtjs/meta": "latest",
    "@nuxtjs/workbox": "latest",
    "axios": "latest",
    "contentful": "latest",
    "contentful-management": "latest",
    "cross-env": "latest",
    "dawa-autocomplete2": "latest",
    "gotrue-js": "latest",
    "hash.js": "latest",
    "lodash": "latest",
    "marked": "latest",
    "netlify-lambda": "latest",
    "nuxt": "latest",
    "vue-lazyload": "latest",
    "vuex-persistedstate": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
